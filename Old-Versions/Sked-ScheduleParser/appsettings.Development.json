{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Kafka": {
    "BootstrapServer" : "localhost:9092",
    "SchedulesTopic": "Schedules"
  },
  "ScheduleParserOptions": {
    "MAIScheduleUrl" : "https://mai.ru/education/studies/schedule"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting": "Information",
        "System": "Warning"
      }
    },
    "WriteTo": [ "Console" ]
  }
  
}
